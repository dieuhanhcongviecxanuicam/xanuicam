# cloudflared config template
# Fill in the placeholders before use. Do NOT commit real credentials.

tunnel: __TUNNEL_ID__
credentials-file: /etc/cloudflared/credentials.json

ingress:
  - hostname: dev.xanuicam.vn
    service: http://backend:5000
  - hostname: www.xanuicam.vn
    service: http://backend:5000
  - service: http_status:404

# Optional: override origin request settings
originRequest:
  connectTimeout: 30s
  noTLSVerify: false
